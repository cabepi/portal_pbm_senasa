{
    "rewrites": [
        {
            "source": "/v1/:path*",
            "destination": "https://dev.api.middleware.koneksi.com.do/v1/:path*"
        },
        {
            "source": "/((?!api/.*).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "maxDuration": 60
        }
    }
}